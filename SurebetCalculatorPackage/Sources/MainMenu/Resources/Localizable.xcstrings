        "el": {
          "stringUnit": {
            "state": "translated",
            "value": "Επιλέξτε μία λειτουργία: από το συνολικό ποντάρισμα, από μία επιλεγμένη έκβαση ή χειροκίνητα."
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "Elija un modo: desde la apuesta total, desde un resultado seleccionado o manual."
