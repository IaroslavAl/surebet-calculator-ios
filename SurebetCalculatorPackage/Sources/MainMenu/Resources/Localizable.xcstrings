    "instructions_step_two_body": {
      "extractionState": "manual",
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Choose one mode: from total stake, from one selected outcome, or manual."
          }
        },
        "ru": {
          "stringUnit": {
            "state": "translated",
            "value": "Выберите один режим: от общей суммы ставок, от выбранного исхода или ручной."
          }
        },
        "de": {
          "stringUnit": {
            "state": "translated",
            "value": "Wählen Sie einen Modus: nach Gesamteinsatz, nach ausgewähltem Ausgang oder manuell."
          }
