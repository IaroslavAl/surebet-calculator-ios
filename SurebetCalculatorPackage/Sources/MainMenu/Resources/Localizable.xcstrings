    "instructions_step_four_body": {
      "extractionState": "manual",
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Tap the trash button in the top bar to clear all entered data."
          }
        },
        "ru": {
          "stringUnit": {
            "state": "translated",
            "value": "Нажмите кнопку корзины в верхней панели, чтобы сбросить все введённые данные."
          }
        },
        "de": {
          "stringUnit": {
            "state": "translated",
            "value": "Tippen Sie auf die Papierkorb-Schaltfläche oben, um alle eingegebenen Daten zu löschen."
          }
        },
        "el": {
          "stringUnit": {
            "state": "translated",
            "value": "Πατήστε το κουμπί κάδου στην επάνω μπάρα για να διαγράψετε όλα τα δεδομένα που εισαγάγατε."
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "Toque el botón de papelera en la barra superior para borrar todos los datos introducidos."
          }
        },
        "fr": {
          "stringUnit": {
            "state": "translated",
            "value": "Touchez le bouton corbeille dans la barre supérieure pour effacer toutes les données saisies."
          }
        },
        "it": {
          "stringUnit": {
            "state": "translated",
            "value": "Tocca il pulsante del cestino nella barra superiore per cancellare tutti i dati inseriti."
          }
        },
        "pt": {
          "stringUnit": {
            "state": "translated",
            "value": "Toque no botão de lixeira na barra superior para limpar todos os dados inseridos."
          }
        }
      }
    },
    "instructions_step_four_title": {
      "extractionState": "manual",
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Clear all fields"
          }
        },
        "ru": {
          "stringUnit": {
            "state": "translated",
            "value": "Очистите все поля"
          }
        },
        "de": {
          "stringUnit": {
            "state": "translated",
            "value": "Alle Felder löschen"
          }
        },
        "el": {
          "stringUnit": {
            "state": "translated",
            "value": "Καθαρίστε όλα τα πεδία"
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "Borre todos los campos"
          }
        },
        "fr": {
          "stringUnit": {
            "state": "translated",
            "value": "Effacez tous les champs"
          }
        },
        "it": {
          "stringUnit": {
            "state": "translated",
            "value": "Cancella tutti i campi"
          }
        },
        "pt": {
          "stringUnit": {
            "state": "translated",
            "value": "Limpe todos os campos"
          }
        }
      }
    },
    "instructions_step_one_body": {
      "extractionState": "manual",
      "localizations": {
        "en": {
          "stringUnit": {
            "state": "translated",
            "value": "Set from 2 to 20 outcomes and enter odds for each outcome."
          }
        },
        "ru": {
          "stringUnit": {
            "state": "translated",
            "value": "Укажите от 2 до 20 исходов и введите коэффициент для каждого исхода."
          }
        },
        "de": {
          "stringUnit": {
            "state": "translated",
            "value": "Wählen Sie 2 bis 20 Ausgänge und geben Sie für jeden Ausgang die Quote ein."
          }
        },
        "el": {
          "stringUnit": {
            "state": "translated",
            "value": "Ορίστε από 2 έως 20 εκβάσεις και εισαγάγετε την απόδοση για καθεμία."
          }
        },
        "es": {
          "stringUnit": {
            "state": "translated",
            "value": "Defina de 2 a 20 resultados e introduzca la cuota para cada resultado."
          }
        },
        "fr": {
          "stringUnit": {
            "state": "translated",
            "value": "Définissez de 2 à 20 issues et saisissez la cote pour chaque issue."
          }
        },
        "it": {
          "stringUnit": {
            "state": "translated",
            "value": "Imposta da 2 a 20 esiti e inserisci la quota per ogni esito."
          }
        },
        "pt": {
          "stringUnit": {
            "state": "translated",
            "value": "Defina de 2 a 20 resultados e insira a odd para cada resultado."
          }
        }
      }
    },
